                        Vector3.new(42, -3, 35),
                        Vector3.new(42, -3, 42),
                        Vector3.new(42, -3, 49),
                        Vector3.new(42, -3, 0),
                        Vector3.new(-42, -3, -7),
                        Vector3.new(-42, -3, -14),
                        Vector3.new(-42, -3, -21),
                        Vector3.new(-42, -3, -28),
                        Vector3.new(-42, -3, -35),
                        Vector3.new(-42, -3, -42),
                        Vector3.new(-42, -3, -49),
                        Vector3.new(-42, -3, 7),
                        Vector3.new(-42, -3, 14),
                        Vector3.new(-42, -3, 21),
                        Vector3.new(-42, -3, 28),
                        Vector3.new(-42, -3, 35),
                        Vector3.new(-42, -3, 42),
                        Vector3.new(-42, -3, 49),
                        Vector3.new(-42, -3, 0),
                    }
                    
                    for _, position in ipairs(coordinates) do
                        Remote(
                            cframe*position,
                            "Plant Box",
                            0,
                            false
                        )
                        task.wait(0.3)
                    end
                end,
                DoubleClick = false,
                Tooltip = 'Creates a farm'
            })
